[{"internal_id": 161363531, "Award ID": "R23AC00141", "Award Amount": 24224.2, "Award Type": "COOPERATIVE AGREEMENT (B)", "Base Obligation Date": "2023-07-19", "CFDA Number": "15.518", "Description": "OPERATION OF THE GDU HAS RESULTED IN ADVERSE IMPACTS TO AUDUBON WILDLIFE MANAGEMENT AREA(WMA). THE GDU REFORMULATION ACT OF 1986 AND THE DAKOTA WATER RESOURCES ACT OF 2000REQUIRE MITIGATION FOR THOSE IMPACTS.THE UNITED STATES, ACTING THROUGH RECLAMATION, IS ACQUIRING TITLE TO CERTAIN LANDS IN THE STATE OFNORTH DAKOTA AS MITIGATION FOR IMPACTS OF THE GARRISON DIVERSION UNIT TO AUDUBON WMA.THE PURPOSE OF THIS AGREEMENT IS TO TRANSFER FUNDS TO THE NDGF DEPARTMENT FOR MANAGEMENT OF2,767 ACRES OF LAND ACQUIRED FOR MITIGATION OF IMPACTS TO AUDUBON WMA. LAND OWNERSHIPREMAINS WITH THE UNITED STATES OF AMERICA. RECLAMATION RETAINS PRIMARY JURISDICTION AND IS THEFEDERAL LAND ADMINISTERING AGENCY.", "Place of Performance Country Code": "USA", "Place of Performance State Code": "ND", "COVID-19 Obligations": null, "Infrastructure Obligations": null, "recipient_id": "746bc880-652f-587c-ca47-7b63d94ad3ff-C", "generated_internal_id": "ASST_NON_R23AC00141_1425"}, {"internal_id": 160633256, "Award ID": "R23AC00093", "Award Amount": 33545000.0, "Award Type": "COOPERATIVE AGREEMENT (B)", "Base Obligation Date": "2023-06-21", "CFDA Number": "15.518", "Description": "THIS AGREEMENT PROVIDES FUNDING FOR CONSTRUCTION AND OR MODIFICATION OF MUNICIPAL, RURAL, AND INDUSTRIAL (MR I) WATER SYSTEMS THROUGH GARRISON DIVERSION CONSERVANCY DISTRICT.  COMPLETED ACTIVITIES WILL PROVIDE WATER TO RESIDENTS THROUGHOUT THE STATE OF NORTH DAKOTA.  GARRISON DIVERSION HAS BEEN AUTHORIZED UNDER STATE LAW AS THE ORGANIZATION TO ADMINISTER RURAL WATER PROJECT FOR THE GARRISON DIVERSION UNIT PROJECT. INDIVIDUAL RURAL WATER ORGANIZATIONS AND THE NORTH DAKOTA DEPARTMENT OF NATURAL RESOURCES, UNDER AGREEMENTS WITH GARRISON DIVERSION, PERFORM THE DIRECT DESIGN AND CONSTRUCTION ACTIVITIES ASSOCIATED WITH THE FUNDING PROVIDED HEREIN.THE RECIPIENT WILL COMPLETE PHASE II OF THE NORTHWEST AREA WATER SUPPLY PROJECT (NAWS) BIOTA WATER TREATMENT PLANT (WTP).  THE PROJECT INCLUDES THE DESIGN AND CONSTRUCTION OF AN ADDITION TO THE PHASE I BIOTA WATER TREATMENT PLANT, WHICH IS CURRENTLY UNDER CONSTRUCTION.  IT WILL ALSO INCLUDE A BUILDING EXPANSION, TREATMENT PROCESS CAPACITY, PUMPING, AND SOLIDS THICKENING AND HANDLING SYSTEM.", "Place of Performance Country Code": "USA", "Place of Performance State Code": "ND", "COVID-19 Obligations": null, "Infrastructure Obligations": 33545000.0, "recipient_id": "6885de62-b15b-888b-857a-d6952ac60b0b-C", "generated_internal_id": "ASST_NON_R23AC00093_1425"}, {"internal_id": 140481999, "Award ID": "R21AP10193", "Award Amount": 4451000.0, "Award Type": "PROJECT GRANT (B)", "Base Obligation Date": "2021-09-23", "CFDA Number": "15.518", "Description": "NORTH DAKOTA NATURAL RESOURCES TRUST ANNUAL CONTRIBUTION", "Place of Performance Country Code": "USA", "Place of Performance State Code": "ND", "COVID-19 Obligations": null, "Infrastructure Obligations": null, "recipient_id": "e40fc5fd-78fb-ce0b-93d2-374920f865e5-C", "generated_internal_id": "ASST_NON_R21AP10193_1425"}, {"internal_id": 68784714, "Award ID": "R18AC00098", "Award Amount": 82155.23, "Award Type": "COOPERATIVE AGREEMENT (B)", "Base Obligation Date": "2018-09-21", "CFDA Number": "15.518", "Description": "DK-AUDUBON WMA MITIGATION", "Place of Performance Country Code": "USA", "Place of Performance State Code": "ND", "COVID-19 Obligations": null, "Infrastructure Obligations": null, "recipient_id": "746bc880-652f-587c-ca47-7b63d94ad3ff-C", "generated_internal_id": "ASST_NON_R18AC00098_1425"}, {"internal_id": 68784558, "Award ID": "R18AC00095", "Award Amount": 4159748.66, "Award Type": "COOPERATIVE AGREEMENT (B)", "Base Obligation Date": "2018-07-19", "CFDA Number": "15.518", "Description": "DK-FA-LONETREE NDGF NEW AGREEMENT", "Place of Performance Country Code": "USA", "Place of Performance State Code