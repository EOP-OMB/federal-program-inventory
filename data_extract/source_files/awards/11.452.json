[{"internal_id": 161919613, "Award ID": "NA23NMF4520292", "Award Amount": 50000.0, "Award Type": "PROJECT GRANT (B)", "Base Obligation Date": "2023-08-10", "CFDA Number": "11.452", "Description": "THE LAKE AND PENINSULA BOROUGH SCHOOL DISTRICT (LPSD), DILLINGHAM CITY SCHOOL DISTRICT (DCSD), BRISTOL BAY BOROUGH SCHOOL DISTRICT (BBBSD), AND THE SOUTHWEST REGION SCHOOL DISTRICT (SWRSD) FORMED A PARTNERSHIP IN 2011 FOR THE DEVELOPMENT AND IMPLEMENTATION OF THE BRISTOL BAY REGION CAREER AND TECHNICAL EDUCATION (BBRCTE) PROGRAM. BBRCTE ACKNOWLEDGES THE DOCUMENTED NEED FOR CAREER AWARENESS AND EDUCATION IN COMMERCIAL FISHING, SUBSISTENCE FISHING, FISHERIES SCIENCE, AND FISHERIES LEADERSHIP FOR STUDENTS IN THE BRISTOL BAY REGION. THROUGH THE FISHERIES PROGRAM, BBRCTE WILL RECRUIT AND EDUCATE LOCAL STUDENTS ON SUBSISTENCE KNOWLEDGE AND THE VERY REAL APPLICATION OF THIS KNOWLEDGE TO THE COMMERCIAL FISHING INDUSTRY. ADDITIONALLY, THE PROGRAM WILL DEVELOP WATERSHED RESIDENT WORKFORCE WHO UNDERSTAND THAT PROFITING OFF OF THE WATERSHED MEANS PRIORITIZING THE ECOLOGICAL HEALTH OF THE AREA AND SUPPORT THE BRISTOL BAY ECONOMY BY RESIDING IN-REGION.", "Place of Performance Country Code": "USA", "Place of Performance State Code": "AK", "COVID-19 Obligations": null, "Infrastructure Obligations": null, "recipient_id": "ccd2d155-14a7-6937-27c8-fe0eac27c78a-C", "generated_internal_id": "ASST_NON_NA23NMF4520292_1330"}, {"internal_id": 161919612, "Award ID": "NA23NMF4520291", "Award Amount": 10000.0, "Award Type": "PROJECT GRANT (B)", "Base Obligation Date": "2023-08-10", "CFDA Number": "11.452", "Description": "THIS PROJECT TO THE ANCHORAGE PARK FOUNDATION WOULD SUPPORT THE YOUTH EMPLOYMENT IN PARKS (YEP) PROGRAM. YEP IS A COMPREHENSIVE AND LIFE-CHANGING PROGRAM THAT REQUIRES MANY PUBLIC AND PRIVATE SUPPORTS TO OFFER MEANINGFUL PROFESSIONAL EXPERIENCE WHILE IMPROVING THE PARKS AND TRAILS THAT MAKE ANCHORAGE A GREAT PLACE TO LIVE, WORK, LEARN, AND VISIT. THIS PROGRAM HELPS ANCHORAGE TEENS FROM PREDOMINANTLY LOW-INCOME AND DIVERSE NEIGHBORHOODS TO BETTER UNDERSTAND LOCAL MARINE RESOURCE ISSUES AND RELATED PROFESSIONS THROUGH FIELD TRIPS, PRESENTATIONS, AND CAREER MENTORING. UNDER THE GUIDANCE OF CAREER PROFESSIONALS, TEENS LEARN ABOUT FORESTRY, WATER RESOURCES, WILDLIFE MANAGEMENT, FISHERIES, LAND MANAGEMENT, AND CLIMATE CHANGE IMPACTS WHILE LEARNING ABOUT BUILDING TRAILS, RESTORING STREAM BANKS, REMOVING INVASIVE PLANT SPECIES, AND REPLANTING NATIVE TREES.", "Place of Performance Country Code": "USA", "Place of Performance State Code": "AK", "COVID-19 Obligations": null, "Infrastructure Obligations": null, "recipient_id": "cbd9866a-62b5-a9dd-33de-ce01b67fcca1-R", "generated_internal_id": "ASST_NON_NA23NMF4520291_1330"}, {"internal_id": 150724815, "Award ID": "NA22NMF4520196", "Award Amount": 10000.0, "Award Type": "PROJECT GRANT (B)", "Base Obligation Date": "2022-07-22", "CFDA Number": "11.452", "Description": "THIS PILOT PROGRAM IS DESIGNED TO ADDRESS THE UNDERREPRESENTATION OF ALASKA NATIVE STUDENTS IN SCIENCE, TECHNOLOGY, ENGINEERING AND MATH FIELDS AND SUPPORT THE DEVELOPMENT OF A DIVERSE FUTURE WORKFORCE FOR NOAA LINE OFFICES IN THE ALASKA REGION. ALASKA FISHERIES SCIENCE CENTER WILL BE PARTNERING WITH STAFF FROM SEALASKA HERITAGE INSTITUTE, THE JUNEAU SCHOOL DISTRICT, CHATHAM SCHOOL DISTRICT, WRANGELL SCHOOL DISTRICT, SITKA SCHOOL DISTRICT, KLAWOCK SCHOOL DISTRICT, HOONAH SCHOOL DISTRICT, AND THE JUNEAU STEM COALITION TO PILOT A REGIONAL SCIENCE FAIR TO BE HELD AT THE UNIVERSITY OF ALASKA SOUTHEAST FOR ALASKA NATIVE HIGH SCHOOL STUDENTS AND RURAL SOUTHEAST ALASKA STUDENTS IN MAY OF 2023. SEALASKA HERITAGE INSTITUTE HAS CONTRACTED WITH SANJAY PYARE PHD AT UNIVERSITY OF ALASKA SOUTHEAST TO START AN AMERICAN INDIAN SCIENCE AND ENGINEERING SOCIETY COLLEGE CHAPTER BEGINNING FALL 2022. THIS CHAPTER SUPPORTS INDIGENOUS PEOPLE IN SCIENCE FIELDS, PROVIDING A NATIONAL SCIENCE FAIR, SCHOLARSHIPS,", "Place of Performance Country Code": "USA", "Place of Performance State Code": "AK", "COVID-19 Obligations": null, "Infrastructure Obligations": null, "recipient_id": "97b8cbcb-ac70-d0ca-fabf-d46725d122de-C", "generated_internal_id": "ASST_NON_NA22NMF4520196_1330"}, {"internal_id": 150724819, "Award ID": "NA22NMF4520195", "Award Amount": 14916.0, "Award Type": "PROJECT GRANT (B)", "Base Obligation Date": "2022-07-20", "CFDA Number": "11.452", "Description": "THE PROPOSED PROJECT WILL INCREASE PARTICIPATION IN MARINE SCIENCE TRAINING BY COASTAL ALASKAN COLLEGE STUDENTS ATTENDING THE UNIVERSITY OF ALASKA SOUTHEAST, A MINORITY SERVING INSTITUTION. STUDENTS ENROLLED IN A SCIENCE DEGREE WILL BE MATCHED WITH MENTORS FROM PARTNER MARINE SCIENCE AND NATURAL RESOURCE MANAGEMENT AGENCIES BASED UPON SHARED INTERESTS AND AVAILABLE RESEARCH PROJECTS. STUDENT-MENTOR PAIRS WILL COMPLETE A RESEARCH PLAN AND THEN PURSUE RESEARCH PROJECTS DURING ONE ACADEMIC YEAR. STUDENTS WILL GIVE RESEARCH PROJECT PRESENTATIONS AT A RESEARCH SYMPOSIUM UPON COMPLETION OF THEIR PROJECTS AT THE END OF THE ACADEMIC YEAR. THE PROPOSED PROJECT WILL PROVIDE MARINE SCIENCE RESEARCH EXPERIENCES FOR COASTAL ALASKAN STUDENTS AND STRENGTHEN UNIVERSITY-AGENCY PARTNERSHIPS, THUS INCREASING OPPORTUNITIES IN MARINE SCIENCE FOR FUTURE STUDENTS.", "Place of Performance Country Code": "USA", "Place of Performance State Code": "AK", "COVID-19 Obligations": null, "Infrastructure Obligations": null, "recipient_id": "ccd87580-335a-2c06-d461-b758bf9ae920-C", "generated_internal_id": "ASST_NON_NA22NMF4520195_1330"}, {"internal_id": 151290806, "Award ID": "NA22NMF4520194", "Award Amount": 15248.0, "Award Type": "PROJECT GRANT (B)", "Base Obligation Date": "2022-08-02", "CFDA Number": "11.452", "Description": "THIS MINI GRANT WILL PROVIDE THREE CORDOVA-BASED MARICULTURISTS WILL TRAVEL TO SEAGROVE KELP COMPANY IN THE FALL OF 2022 AND HOLD THE FIRST &QUOT;KELP THE SOUND&QUOT; WORKSHOP IN CORDOVA, ALASKA TO MEET THE FOLLOWING OBJECTIVES: 1) LEARN FROM AN ALASKAN KELP NURSERY AND FARMERS ABOUT SITE SELECTION, SEED COLLECTION, NURSERY OPERATIONS, AND OUT PLANTING YOUNG KELP AND 2) SHARE INFORMATION AND RESOURCES WITH RESIDENTS OF PRINCE WILLIAM SOUND. AT THE END OF THIS PROJECT, WE WILL HAVE THE FOLLOWING OUTCOMES BY THE APRIL 1, 2023: 1) A FREE, HANDS-ON WORKSHOP ABOUT KELP FARMING HELD IN CORDOVA, ALASKA; 2) SURVEY FEEDBACK, NOTES FROM LISTENING SESSIONS, AND A LIST OF TOPIC AREAS FOR FUTURE WORKSHOPS; 3) A DESIGN AND STANDARD OPERATING PROCEDURE FOR A NEW PWS-BASED KELP NURSERY.", "Place of Performance Country Code": "USA", "Place of Performance State Code": "AK", "COVID-19 Obligations": null, "Infrastructure Obligations": null, "recipient_id": "f2b9855f-a835-7615-1311-d2fc26f60bc2-C", "generated_internal_id": "ASST_NON_NA22NMF4520194_1330"}, {"internal_id": 139144891, "Award ID": "NA21NMF4520549", "Award Amount": 11750.0, "Award Type": "PROJECT GRANT (B)", "Base Obligation Date": "2021-08-13", "CFDA Number": "11.452", "Description": "ORAL HISTORY DOCUMENTATION OF FISHPOND AND AQUACULTURE PRACTITIONERS IN HAWAII", "Place of Performance Country Code": "USA", "Place of Performance State Code": "HI", "COVID-19 Obligations": null, "Infrastructure Obligations": null, "recipient_id": "19d99269-b976-8845-8df7-0cf35aaa9a21-R", "generated_internal_id": "ASST_NON_NA21NMF4520549_1330"}, {"internal_id": 138797608, "Award ID": "NA21NMF4520517", "Award Amount": 15000.0, "Award Type": "PROJECT GRANT (B)", "Base Obligation Date": "2021-07-20", "CFDA Number": "11.452", "Description": "HAWAIIAN COMMUNITY REEF FISH COOKBOOK-FOOD, SCIENCE, & TRADITION", "Place of Performance Country Code": "USA", "Place of Performance State Code": "HI", "COVID-19 Obligations": null, "Infrastructure Obligations": null, "recipient_id": "ae29ed7c-2499-1d74-9bcc-d147785e2826-C", "generated_internal_id": "ASST_NON_NA21NMF4520517_1330"}, {"internal_id": 139144894, "Award ID": "NA21NMF4520516", "Award Amount": 14998.0, "Award Type": "PROJECT GRANT (B)", "Base Obligation Date": "2021-08-