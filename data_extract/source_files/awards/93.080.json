[{"internal_id": 127716191, "Award ID": "NU58DD000023", "Award Amount": 605422.0, "Award Type": "COOPERATIVE AGREEMENT (B)", "Base Obligation Date": "2021-02-24", "CFDA Number": "93.080", "Description": "CDC-RFA-DD20-2003: VIRGINIA SICKLE CELL DATA COLLECTION PROGRAM - 2020", "Place of Performance Country Code": "USA", "Place of Performance State Code": "VA", "COVID-19 Obligations": null, "Infrastructure Obligations": null, "recipient_id": "81307f03-67cd-51c5-75a3-b43876b0cc3b-C", "generated_internal_id": "ASST_NON_NU58DD000023_7523"}, {"internal_id": 127716182, "Award ID": "NU58DD000022", "Award Amount": 610119.0, "Award Type": "COOPERATIVE AGREEMENT (B)", "Base Obligation Date": "2021-02-24", "CFDA Number": "93.080", "Description": "CDC-RFA-DD20-2003: THE COLORADO SICKLE CELL SURVEILLANCE PROJECT - THE COLORADO SICKLE CELL SURVEILLANCE PROJECT AIMS TO INCREASE THE UNDERSTANDING OF THE EPIDEMIOLOGY AND HEALTHCARE UTILIZATION PATTERNS OF INDIVIDUALS WITH SICKLE CELL DISEASE IN COLORADO.   OUR COLLABORATIVE EFFORT WILL INCORPORATE AN EXISTING COMPREHENSIVE DATA WAREHOUSE THAT INTEGRATES CLINICAL DATA FROM THE PRIMARY PEDIATRIC AND ADULT SICKLE CELL TREATMENT CENTERS WITH VITAL RECORDS AND ALL-PAYER CLAIMS DATA. ADDITIONAL CLINICAL DATA FOR INDIVIDUALS NOT IN THE SYSTEM WILL BE ASCERTAINED THROUGH ALL-PAYER CLAIMS DATA, INCLUDING MEDICAID AND MEDICARE, NEWBORN SCREENING RECORDS, AND DEATH RECORDS. A MULTIDISCIPLINARY APPROACH WILL BE UTILIZED TO REFINE STRATEGIES FOR DATA ACQUISITION AND MERGING, AND DEVELOP APPROACHES FOR EDUCATION AND DISSEMINATION OF INFORMATION WITHIN COLORADO AND ACROSS THE U.S.  KEY OUTCOMES ARE THE INCREASED UNDERSTANDING OF THE EPIDEMIOLOGY AND HEALTH CARE UTILIZATION PATTERNS OF SICKLE CELL DISEASE IN COLORADO, THE STATE WITH THE LARGEST SICKLE CELL POPULATION IN THE ROCKY MOUNTAIN REGION.", "Place of Performance Country Code": "USA", "Place of Performance State Code": "CO", "COVID-19 Obligations": null, "Infrastructure Obligations": null, "r