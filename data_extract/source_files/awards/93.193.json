[{"internal_id": 49212994, "Award ID": "NU17CE924846", "Award Amount": 2229760.9, "Award Type": "COOPERATIVE AGREEMENT (B)", "Base Obligation Date": "2016-06-28", "CFDA Number": "93.193", "Description": "KENTUCKY VIOLENCE AND INJURY PREVENTION PROGRAM", "Place of Performance Country Code": "USA", "Place of Performance State Code": "KY", "COVID-19 Obligations": 171244.64, "Infrastructure Obligations": null, "recipient_id": "c477975b-68db-c3dc-4a79-b95cb9f6ab3e-C", "generated_internal_id": "ASST_NON_NU17CE924846_7523"}, {"internal_id": 149209882, "Award ID": "H723IHS0007", "Award Amount": 3700000.0, "Award Type": "COOPERATIVE AGREEMENT (B)", "Base Obligation Date": "2022-05-24", "CFDA Number": "93.193", "Description": "URBAN INDIAN EDUCATION AND RESEARCH PROGRAM", "Place of Performance Country Code": "USA", "Place of Performance State Code": "DC", "COVID-19 Obligations": 1000000.0, "Infrastructure Obligations": null, "recipient_id": "61a97632-2f2e-d295-74b6-3984e54d6c40-C", "generated_internal_id": "ASST_NON_H723IHS0007_7527"}, {"internal_id": 127830488, "Award ID": "H723IHS0006", "Award Amount": 225000.0, "Award Type": "COOPERATIVE AGREEMENT (B)", "Base Obligation Date": "2021-03-01", "CFDA Number": "93.193", "Description": "NCUIH NATIONAL URBAN INDIAN BEHAVIORAL HEALTH AWARENESS PROJECT - ABSTRACTAMERICAN INDIANS AND ALASKA NATIVES (AI/AN) THAT LIVE IN URBAN AREAS PRESENT WITH A UNIQUE MORBIDITY AND MORTALITY PROFILE, AND THEY ENDURE A DISPROPORTIONATE RATE OF BEHAVIORAL HEALTH ISSUES.   THE MOST SIGNIFICANT MENTAL HEALTH CONCERNS FACING URBAN AI/ANS INCLUDE A HIGH PREVALENCE OF DEPRESSION, SUBSTANCE USE DISORDERS, SUICIDE, ANXIETY, AND PTSD. SUICIDE IS THE SECOND LEADING CAUSE OF DEATH OF URBAN INDIAN YOUTH. MAINSTREAM HEALTHCARE SYSTEMS AND PROVIDERS ARE OFTEN NOT ABLE TO MEET URBAN AI/AN MORBIDITY, MORTALITY, AND PUBLIC HEALTH CONCERNS, PARTICULARLY IN A CULTURALLY SENSITIVE MANNER.THROUGH THE NATIONAL URBAN INDIAN BEHAVIORAL HEALTH AWARENESS (NUIBHA) PROGRAM, THE NATIONAL COUNCIL OF URBAN INDIAN HEALTH (NCUIH) PROPOSES TO PROVIDE A NATIONAL INFORMATION-SHARING PROGRAM BY WHICH THE AWARENESS, VISIBILITY, ADVOCACY, AND EDUCATION FOR BEHAVIORAL HEALTH ISSUES OF URBAN INDIAN HEALTH CARE CAN BE IMPROVED AND ACHIEVE A NATIONAL PRIORITY. AS THE ONLY NATIONAL NONPROFIT DEDICATED TO THE SUPPORT AND DEVELOPMENT OF QUALITY, ACCESSIBLE, AND CULTURALLY-COMPETENT HEALTH SERVICES FOR AI/AN LIVING IN URBAN SETTINGS, NCUIH RECOGNIZES THE NEED TO PROVIDE EDUCATION, TECHNICAL SUPPORT, AND TRAINING TO URBAN INDIAN ORGANIZATIONS (UIOS) TO ADDRESS ONGOING BEHAVIORAL HEALTH DISPARITIES AND ACHIEVE HEALTHY PEOPLE 2030 GOALS.NCUIH IS WELL-POSITIONED TO ADVANCE BEHAVIORAL HEALTH OUTCOMES WITHIN THE URBAN INDIAN POPULATION BY ENGAGING THE 41 UIOS THAT PROVIDE CULTURALLY APPROPRIATE HEALTH SERVICES TO AI/AN PEOPLE LIVING IN URBAN AREAS. NUIBHA, WITH ITS COMPREHENSIVE NATIONAL PROJECT APPROACH, WILL ENGAGE UIOS IN STRATEGIC ACTIVITIES THAT OFFER A PLATFORM TO VOICE COMMON CONCERNS, RAISE AWARENESS OF THE POPULATION?S BEHAVIORAL HEALTH DISPARITIES ON A NATIONAL LEVEL, AND PROMOTE THE DISSEMINATION AND SHARING OF BEST PRACTICES, RESOURCES, AND TECHNIQUES. NCUIH AIMS TO PROVIDE TECHNICAL ASSISTANCE AND RESOURCES THAT ARE CULTURALLY RELEVANT, CULTURALLY COMP ETENT, AND TAILORED TO THE SITUATIONAL AND CONTEXTUAL NEEDS OF BEHAVIORAL HEALTH WITHIN THE URBAN INDIAN COMMUNITY THROUGH TECHNICAL ASSISTANCE AND A COMMUNITY OF LEARNING SERIES.  THE COMMUNITY OF LEARNING MODEL IS A PEER-TO-PEER RESOURCE SHARING METHOD THAT PROVIDES UIOS WITH AN OPPORTUNITY TO LEARN FROM WITHIN THE URBAN COMMUNITY OF INNOVATIVE PRACTICES, PREVENTION EFFORTS, GRANT WRITING AND STRATEGIC PLANNING BEST PRACTICES, TO IMPROVE BEHAVIORAL HEALTH PROGRAMS AND PATIENT SERVICES AT FACILITIES SERVING URBAN AI/AN THROUGHOUT THE COUNTRY. A NATIONAL BEHAVIORAL HEALTH LISTENING SESSION WILL BE PROVIDED FOR URBAN AI/AN TO NOT ONLY BRING FORTH CHALLENGES, BUT DETERMINE INNOVATIVE SOLUTIONS THROUGH DISCUSSIONS WITH BEHAVIORAL HEALTH STAFF AND EXPERTS IN THE FIELD, LEADING TO INFORMED ADVOCACY TOWARDS BEHAVIORAL HEALTH CARE FOR URBAN AI/AN.  THIS NATIONAL BEHAVIORAL HEALTH AWARENESS PROGRAM WILL PROVIDE A PLATFORM AND MECHANISM TO EXAMINE AND PRIORITIZE QUALITY PREVENTION, TREATMENT, AND RECOVERY ELEMENTS AT THE URBAN AI/AN POPULATION LEVEL. IT SUPPORTS BETTER CARE AND HEALTHIER COMMUNITIES THROUGH THE DISSEMINATION OF PROVEN INTERVENTIONS AND PRACTICES. THE PROGRAM WILL INCREASE AWARENESS, VISIBILITY, ADVOCACY, AND EDUCATION FOR BEHAVIORAL HEALTH ISSUES ON A NATIONAL SCALE FOR UIOS AND THE URBAN AI/AN PEOPLE THEY SERVE.", "Place of Performance Country Code": "USA", "Place of Performance State Code": null, "COVID-19 Obligations": null, "Infrastructure Obligations": null, "recipient_id": "61a97632-2f2e-d295-74b6-3984e54d6c40-C", "generated_internal_id": "ASST_NON_H723IHS0006_7527"}, {"internal_id": 48833697, "Award ID": "H723IHS0004", "Award Amount": 220904.63, "Award Type": "COOPERATIVE AGREEMENT (B)", "Base Obligation Date": "2017-08-07", "CFDA Number": "93.193", "Description": "NCUIH NATIONAL URBAN INDIAN BEHAVIORAL HEALTH AWARENESS", "Place of Performance Country Code": "USA", "Place of Performance State Code": "DC", "COVID-19 Obligations": null, "Infrastructure Obligations": null, "recipient_id": "61a97632-2f2e-d295-74b6-3984e54d6c40-C", "generated_internal_id": "ASST_NON_H723IHS0004_7527"}, {"internal_id": 48833696, "Award ID": "H723IHS0003", "Award Amount": 8359192.0, "Award Type": "COOPERATIVE AGREEMENT (B)", "Base Obligation Date": "2016-09-09", "CFDA Number": "93.193", "Description": "NCUIH- URBAN INDIAN EDUCATION AND RESEARCH ORGANIZATION COOPERATIVE AGREEMENT PROGRAM TO SUPPORT URBAN INDIAN HEALTH ORGANIZATIONS THROUGH EDUCATION,", "Place of Performance Country Code": "USA", "Place of Performance State Code": "DC", "COVID-19 Obligations": 2000000.0, "Infrastructure Obligations": null, "recipient_id": "61a97632-2f2e-d295-74b6-3984e54d6c40-C", "generated_internal_id": "ASST_NON_H723IHS0003_7527"}, {"internal_id": 147540977, "Award ID": "H722IHS0278", "Award Amount": 422410.0, "Award Type": "PROJECT GRANT (B)", "Base Obligation Date": "2022-03-29", "CFDA Number": "93.193", "Description": "BUTTE NATIVE WELLNESS CENTER 4-IN-1 PROJECT - THE BUTTE NATIVE WELLNESS CENTER (BNWC) IS CLASSIFIED AS AN URBAN INDIAN ORGANIZATION (UIO) AND IS DESIGNATED AS A FEDERALLY QUALIFIED HEALTH CENTER LOOK-A-LIKE THAT PROVIDES COMPREHENSIVE PRIMARY CARE AND RELATED SERVICES TO ALASKA NATIVES (AI/AN) LIVING WITHIN THE BUTTE SILVER-BOW CATCHMENT AREA (6 COUNTIES). BNWC IS PROPOSING A 5-YEAR, IHS 4-IN-1 PROJECT THAT WILL ACCOMPLISH THE FOLLOWING GOALS AND OBJECTIVES:  HP/DP GOAL: \u201cAI/AN INDIVIDUALS WILL BE ENGAGED AND EMPOWERED TO CHOOSE HEALTHY BEHAVIORS AND MAKE CHANGES THAT REDUCE THE RISK OF DEVELOPING CHRONIC DISEASES AND OTHER MORBIDITIES. THIS GOAL WILL CONTINUE FOR EACH PROJECT YEAR. OBJECTIVE 1: A HEALTH SURVEY WILL BE COMPLETED IN YEAR 1 AND DATA UPDATED EACH YEAR THEREAFTER. OBJECTIVE 2: BNWC WILL CONDUCT AN OUTREACH AND EDUCATION CAMPAIGN. OBJECTIVE 3: BNWC WILL IMPLEMENT DISEASE PREVENTION EFFORTS AND INCREASE PARTICIPATION BY 10% EACH YEAR. OBJECTIVE 4: BNWC WILL CONDUCT A 10-YEAR ASCVD RISK ASSESSMENT ON AT LEAST 50 ADULTS. IMMUNIZATION GOAL: \u201cREDUCE THE INCIDENCE OF VACCINE PREVENTABLE DISEASES OF AI/ANS OF ALL AGE GROUPS IN OUR CATCHMENT AREA.\u201d THIS GOAL WILL CONTINUE FOR EACH PROJECT YEAR. OBJECTIVE 1: BNWC WILL CONDUCT SERVICE IMMUNIZATION SCREENINGS OF ALL PATIENTS. OBJECTIVE 2: BNWC WILL ESTABLISH AND PARTICIPATE IN THE VFC AND ADULT VACCINE PROGRAMS. OBJECTIVE 3: BNWC WILL GENERATE REMINDER/RECALL LETTERS TO UNVACCINATED CHILDREN. OBJECTIVE 4: BNWC WILL CONDUCT RPMS TRAINING SESSIONS TO ALL RELEVANT STAFF. ALCOHOL & SUBSTANCE ABUSE GOAL: \u201cALL PEOPLE WHO SEEK SERVICES AT BNWC WILL HAVE ACCESS TO INDIGENOUS HARM REDUCTION AND WILL BE SUPPORTED BY OTHER PROVIDERS IN THE COMMUNITY TO ACCESS THOSE SERVICES.\u201d THIS GOAL WILL CONTINUE FOR EACH PROJECT YEAR. OBJECTIVE 1: BNWC WILL ESTABLISH AN INDIGENOUS HARM REDUCTION INITIATIVE. OBJECTIVE 2: BNWC WILL CONDUCT TRAININGS ON NALOXONE AND INCREASE PARTICIPATION AMONG PATIENTS BY 10% EACH YEAR. OBJECTIVE 3: BNWC WILL PROVIDE CULTURE AS MEDICINE ACTIVITIES INCREASE PARTICIPATION AMONG PATIENTS BY 10% EACH YEAR. MENTAL HEALTH GOAL: \u201cALL PEOPLE WHO SEEK SERVICES AT BNWC WILL HAVE ACCESS TO MENTAL HEALTH CARE THAT IS GROUNDED IN CULTURE, ADDRESSES DEPRESSION AND MAKES SUICIDE PREVENTION A PRIORITY.\u201d THIS GOAL WILL CONTINUE FOR EACH PROJECT YEAR. OBJECTIVE 1: BNWC WILL CONDUCT PHQ 9 SURVEYS TO ALL PATIENTS AND INCREASE THE NUMBER OF 15% EACH YEAR. OBJECTIVE 2: BNWC WILL INITIATE REFERRALS TO THE CULTURE AS MEDICINE PROGRAMMING AND INCREASE BY 15% EACH YEAR. OBJECTIVE 3: BNWC WILL INITIATE A QPR INITIATIVE AND INCREASE PARTICIPATION BY 10% EACH YEAR.  EVALUATION METHODOLOGY: BNWC WILL COMMISSION RJS & ASSOCIATES, INC. (RJS) TO CONDUCT AN EXTERNAL EVALUATION. THE PURPOSE OF THE EXTERNAL EVALUATION WILL BE TO VERIFY THAT ALL GOALS AND OBJECTIVES ARE BEING COMPLETED WITHIN THE TIMEFRAMES PROPOSED AND PROVIDE RECOMMENDATIONS FOR CONTINUOUS IMPROVEMENT (SEMI-ANNUALLY & ANNUALLY IN YEAR 2) IF NEEDED. THE EXTERNAL EVALUATION WILL BE CONDUCTED AFTER MONTH 6 AND MONTH 12 OF EACH PROJECT PERIOD AND WILL BE IN REPORT FORM.", "Place of Performance Country Code": "USA", "Place of Performance State Code": "MT", "COVID-19 Obligations": null, "Infrastructure Obligations": null, "recipient_id": "b3a04143-d0bf-ad7f-92ce-5ac49fbcf7b6-C", "generated_internal_id": "ASST_NON_H722IHS0278_7527"}, {"internal_id": 89521309, "Award ID": "H722IHS0272", "Award Amount": 633363.0, "Award Type": "PROJECT GRANT (B)", "Base Obligation Date": "2019-06-20", "CFDA Number": "93.193", "Description": "4-IN-1 HEALTH EDUCATION, PREVENTION, AND TREATMENT PROJECT FOR URBAN INDIANS IN BALTIMORE, MARYLAND AND BOSTON, MASSACHUSETTS", "Place of Performance Country Code": "USA", "Place of Performance State Code": "MD", "COVID-19 Obligations": null, "Infrastructure Obligations": null, "recipient_id": "3d2d0dd6-48d9-eabb-6bcc-4ba18f5d0122-C", "generated_internal_id": "ASST_NON_H722IHS0272_7527"}, {"internal_id": 89521283, "Award ID": "H722IHS0271", "Award Amount": 1041362.0, "Award Type": "PROJECT GRANT (B)", "Base Obligation Date": "2019-06-20", "CFDA Number": "93.193", "Description": "NAC 4-IN-1 PROJECT IN MARICOPA COUNTY", "Place of Performance Country Code": "USA", "Place of Performance State Code": "AZ", "COVID-19 Obligations": null, "Infrastructure Obligations": null, "recipient_id": "66e577c1-9d32-b8ed-e4f0-42d14047ec2f-C", "generated_internal_id": "ASST_NON_H722IHS0271_7527"}, {"internal_id": 89522361, "Award ID": "H722IHS0259", "Award Amount": 1022082.0, "Award Type": "PROJECT GRANT (B)", "Base Obligation Date": "2019-04-01", "CFDA Number": "93.193", "Description": "NEW YORK INDIAN COUNCIL 4-IN-1 URBAN HEALTH PROGRAM PROPOSAL.", "Place of Performance Country Code": "USA", "Place of Performance State Code": "NY", "COVID-19 Obligations": null, "Infrastructure Obligations": null, "recipient_id": "d2f32a9b-072b-def6-a94a-4dc4732c18dd-C", "generated_internal_id": "ASST_NON_H722IHS0259_7527"}, {"internal_id": 89521305, "Award ID": "H722IHS0247", "Award Amount": 1023052.0, "Award Type": "PROJECT GRANT (B)", "Base Obligation Date": "2019-03-29", "CFDA Number": "93.193", "Description": "BILLINGS URBAN INDIAN HEALTH AND WELLNESS CENTER 4 IN 1 GRANT", "Place of Performance Country Code": "USA", "Place of Performance State Code": "MT", "COVID-19 Obligations": null, "Infrastructure Obligations": null, "recipient_id": "e520c387-6310-4d65-5351-a4da1742381d-C", "generated_internal_id": "ASST_NON_H722IHS0247_7527"}, {"internal_id": 48833690, "Award ID": "H722IHS0210", "Award Amount": 1535911.0, "Award Type": "PROJECT GRANT (B)", "Base Obligation Date": "2015-03-31", "CFDA Number": "93.193", "Description": "URBAN INDIAN HEALTH PROG